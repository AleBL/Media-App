{
  "name": "media-app",
  "version": "0.1.0",
  "description": "An Electron + Vue 3 starter project setup with Vite, Typescript, Vue-Router, Pinia, Eslint, Prettier, Tailwind CSS",
  "repository": "https://github.com/AleBL/Media-App",
  "author": {
    "name": "Alessandro",
    "url": "https://github.com/AleBL"
  },
  "license": "MIT",
  "private": true,
  "main": "dist-electron/main/index.js",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build:win": "vue-tsc --noEmit && vite build && electron-builder --win",
    "build:mac": "vue-tsc --noEmit && vite build && electron-builder --mac",
    "build:linux": "vue-tsc --noEmit && vite build && electron-builder --linux",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "deps:update": "taze major -I"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vueuse/core": "^9.10.0",
    "autoprefixer": "^10.4.13",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.25.0",
    "pinia": "^2.0.28",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "sass": "^1.76.0",
    "tailwindcss": "^3.2.4",
    "taze": "^0.8.5",
    "typescript": "^4.9.4",
    "unplugin-auto-import": "^0.12.1",
    "vite": "^4.0.4",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.11.4",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.22"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "primeicons": "^7.0.0",
    "primevue": "^3.52.0",
    "yarn": "^1.22.22"
  }
}
