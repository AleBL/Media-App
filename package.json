{
  "name": "media-app",
  "version": "0.1.0",
  "description": "An Electron + Vue 3 starter project setup with Vite, Typescript, Vue-Router, Pinia, Eslint, Prettier, Tailwind CSS",
  "repository": "https://github.com/AleBL/Media-App",
  "author": {
    "name": "Alessandro",
    "url": "https://github.com/AleBL"
  },
  "license": "MIT",
  "private": true,
  "main": "dist-electron/main/index.js",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build:win": "vue-tsc --noEmit && vite build && electron-builder --win",
    "build:mac": "vue-tsc --noEmit && vite build && electron-builder --mac",
    "build:linux": "vue-tsc --noEmit && vite build && electron-builder --linux",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "deps:update": "taze major -I"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vueuse/core": "^10.9.0",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "electron": "^30.0.7",
    "electron-builder": "^24.13.3",
    "esbuild": "^0.21.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.25.0",
    "pinia": "^2.1.7",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "sass": "^1.77.2",
    "tailwindcss": "^3.4.3",
    "taze": "^0.13.8",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.6",
    "vite": "^5.2.11",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.27",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2",
    "vue-tsc": "^2.0.19"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "primeicons": "^7.0.0",
    "primevue": "^3.52.0",
    "yarn": "^1.22.22"
  }
}
